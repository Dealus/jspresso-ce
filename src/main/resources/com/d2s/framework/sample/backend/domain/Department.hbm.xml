<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.d2s.framework.sample.backend.domain" default-access="com.d2s.framework.model.persistence.hibernate.property.EntityPropertyAccessor">
  <class dynamic-update="true" table="DEPARTMENT" persister="com.d2s.framework.model.persistence.hibernate.entity.persister.EntityProxyJoinedSubclassEntityPersister" name="com.d2s.framework.sample.backend.domain.Department" dynamic-insert="true">
    <id type="string" column="ID" length="36" name="id">
      <generator class="assigned"/>
    </id>
    <version column="VERSION" unsaved-value="null" name="version"/>
    <many-to-one cascade="none" name="manager">
      <column name="MANAGER_ID"/>
    </many-to-one>
    <set inverse="true" cascade="persist,merge,save-update,refresh,evict,replicate,delete" name="departmentEmployees">
      <key column="DEPARTMENT_ID"/>
      <one-to-many class="com.d2s.framework.sample.backend.domain.Employee"/>
    </set>
    <set inverse="true" cascade="persist,merge,save-update,refresh,evict,replicate,delete" name="departmentProjects">
      <key column="DEPARTMENT_ID"/>
      <one-to-many class="com.d2s.framework.sample.backend.domain.Project"/>
    </set>
    <property name="name">
      <column name="NAME" not-null="true" length="15"/>
    </property>
    <property name="createTimestamp" type="timestamp">
      <column name="CREATE_TIMESTAMP"/>
    </property>
    <property name="lastUpdateTimestamp" type="timestamp">
      <column name="LAST_UPDATE_TIMESTAMP"/>
    </property>
    <property name="description">
      <column name="DESCRIPTION" length="256"/>
    </property>
  </class>
</hibernate-mapping>
