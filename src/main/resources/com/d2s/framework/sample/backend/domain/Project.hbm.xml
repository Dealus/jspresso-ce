<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.d2s.framework.sample.backend.domain" default-access="com.d2s.framework.model.persistence.hibernate.property.EntityPropertyAccessor">
  <class dynamic-update="true" table="PROJECT" persister="com.d2s.framework.model.persistence.hibernate.entity.persister.EntityProxyJoinedSubclassEntityPersister" name="com.d2s.framework.sample.backend.domain.Project" dynamic-insert="true">
    <id type="string" column="ID" length="36" name="id">
      <generator class="assigned"/>
    </id>
    <version column="VERSION" unsaved-value="null" name="version"/>
    <many-to-one cascade="persist,merge,save-update" name="department">
      <column name="DEPARTMENT_ID"/>
    </many-to-one>
    <set table="PROJECT_PROJECT_MEMBERS" cascade="persist,merge,save-update,refresh,evict,replicate,delete" name="projectMembers">
      <key column="PROJECT_ID"/>
      <many-to-many column="EMPLOYEE_ID" class="com.d2s.framework.sample.backend.domain.Employee"/>
    </set>
    <property name="name">
      <column name="NAME" not-null="true" length="15"/>
    </property>
    <property name="createTimestamp" type="timestamp">
      <column name="CREATE_TIMESTAMP"/>
    </property>
    <property name="lastUpdateTimestamp" type="timestamp">
      <column name="LAST_UPDATE_TIMESTAMP"/>
    </property>
  </class>
</hibernate-mapping>
