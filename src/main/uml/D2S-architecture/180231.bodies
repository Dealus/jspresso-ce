class GatesListener
!!!341511.java!!!	GatesListener(inout action : IAction, inout gates : Collection<IGate>)

      this.action = action;
      this.gates = gates;
      for (IGate gate : gates) {
        gate.addPropertyChangeListener(IGate.OPEN_PROPERTY, this);
      }
!!!341639.java!!!	propertyChange(inout evt : PropertyChangeEvent) : void

      action.setEnabled(GateHelper.areGatesOpen(gates));
